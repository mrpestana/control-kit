**
** ASW Analog Switch object
**
** out = s1 ? in2 : in1
**
@niagaraIcon="module://icons/x16/control/util/switch.png"
class ASW
  extends Component
{
  @readonly property float out
  property float in1
  property float in2
  @allowNull=false property bool  s1

  virtual override void changed(Slot slot)
  {
    super.changed(slot);
    if(slot.name != "out")
      out = s1 ? in2 : in1
  }

}

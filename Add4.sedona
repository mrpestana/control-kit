**
** Add4 Four Input Add object
**
** out = in1 + in2 + in3 + in4
**
class Add4
  extends Add2
{
  property float in3
  property float in4

  virtual override void changed(Slot slot)
  {
    super.changed(slot);
    if(slot.name != "out"){
      out = in1 + in2 + in3 + in4;
    }
  }

}
